            <MainSource>
              <DbSource ConnectionRef="ReportesConnectionString (Web.config)" DbObjectName="Reportes2.dbo.mlocal" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select nombre ,''Descripcion,null Total,null Tiempo,null Atrasado,null promedio,null milisPromedio,null milisTiempo,null promedioTiempo,null milisAtrasado,null promedioAtrasado
		,-1 orden ,0 orden2
	from mlocal where id_local = @idSucursal
	union all
	select emp, Descripcion,sum(tiempo)+sum(atrasado) Total, sum(tiempo) Tiempo,sum(atrasado) Atrasado
		,convert(varchar,(avg(milis)/(1000))/(60*60)) + ':' +(case when (avg(milis)/(1000))/60 &gt; 10 then convert(varchar,(avg(milis)/(1000))/60) else '0'+convert(varchar,(avg(milis)/(1000))/60)  end )+ + ':' 
		+(case when (avg(milis)/(1000))%60 &gt; 10 then convert(varchar,(avg(milis)/(1000))%60) else '0'+convert(varchar,(avg(milis)/(1000))%60) end)promedio
		,avg(milis)milisPromedio
		,isnull(avg(case when milis &lt;= 480000 then milis end),0)milisTiempo
		,isnull(convert(varchar,(avg(case when milis &lt;= 480000 then milis end)/(1000))/(60*60)) + ':' +(case when (avg(case when milis &lt;= 480000 then milis end)/(1000))/60 &gt; 10 then convert(varchar,(avg(case when milis &lt;= 480000 then milis end)/(1000))/60) else '0'+convert(varchar,(avg(case when milis &lt;= 480000 then milis end)/(1000))/60)  end )+ + ':' 
		+(case when (avg(case when milis &lt;= 480000 then milis end)/(1000))%60 &gt; 10 then convert(varchar,(avg(case when milis &lt;= 480000 then milis end)/(1000))%60) else '0'+convert(varchar,(avg(case when milis &lt;= 480000 then milis end)/(1000))%60) end),0)promedioTiempo
		,avg(case when milis &gt; 480000 then milis end)milisAtrasado
		,isnull(convert(varchar,(avg(case when milis &gt; 480000 then milis end)/(1000))/(60*60)) + ':' +(case when (avg(case when milis &gt; 480000 then milis end)/(1000))/60 &gt; 10 then convert(varchar,(avg(case when milis &gt; 480000 then milis end)/(1000))/60) else '0'+convert(varchar,(avg(case when milis &gt; 480000 then milis end)/(1000))/60)  end )+ + ':' 
		+(case when (avg(case when milis &gt; 480000 then milis end)/(1000))%60 &gt; 10 then convert(varchar,(avg(case when milis &gt; 480000 then milis end)/(1000))%60) else '0'+convert(varchar,(avg(case when milis &gt; 480000 then milis end)/(1000))%60) end),0)promedioAtrasado
		,id_emp orden,case when Descripcion = 'Desayuno' then 1 else 2 end
	--into #temp
	from(
		select 'Desayuno' Descripcion,case when DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )&lt;= 480000 then 1 else 0 end tiempo
		,case when DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )&gt; 480000 then 1 else 0 end atrasado	
		,DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )milis,isnull(emp,'') emp,id_emp
		from   hcta a 		
		where 
			DATEADD(day, DATEDIFF(dd, 0,fecha_cierre), 0) between @fecha and @fechaFinal
			and fecha_cierre &lt; CONVERT(datetime,(CONVERT(VARCHAR(15),fecha_cierre,23)  + ' ' + cast(11 as varchar(20))+':59:59')) 
			AND id_local in (select id_local from mlocal where id_local = @idSucursal) 
				 or  id_local in  (select b.idLocal from relacionSucursalUsuario a join RelacionGrupoSucursal b on a.id_local = b.idGrupo 
				 where a.id_usuario = @idUsuario ) 
				 or (a.id_local in (select id_local from mlocal where nombre not like '%trial%' ) and @idSucursal=0 and (select rol from usuarios where id = @idUsuario)='superusuario')
		union all
		select 'Comida' Descripcion,case when DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )&lt;= 480000 then 1 else 0 end tiempo--,hora_bump,cast( fecha_cierre as time)
		,case when DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )&gt; 480000 then 1 else 0 end atrasado
		,DATEDIFF(MILLISECOND,CONVERT(VARCHAR(15),fecha_cierre,114),CONVERT(VARCHAR(15),hora_bump2,114) )milis,isnull(emp,'') emp,id_emp
		from   hcta a
		where 
			DATEADD(day, DATEDIFF(dd, 0,fecha_cierre), 0) between @fecha and @fechaFinal
			and fecha_cierre &gt; CONVERT(datetime,(CONVERT(VARCHAR(15),fecha_cierre,23)  + ' ' + cast(11+1 as varchar(20))+':59:59')) 
			AND id_local in (select id_local from mlocal where id_local = @idSucursal) 
				 or  id_local in  (select b.idLocal from relacionSucursalUsuario a join RelacionGrupoSucursal b on a.id_local = b.idGrupo 
				 where a.id_usuario = @idUsuario ) 
				 or (a.id_local in (select id_local from mlocal where nombre not like '%trial%' ) and @idSucursal=0 and (select rol from usuarios where id = @idUsuario)='superusuario')
		)t0
	group by Descripcion,emp,id_emp
	union all
	select emp,'Total',sum(Total),sum(Tiempo),sum(Atrasado) 
		,convert(varchar,(avg(milisPromedio)/(1000))/(60*60)) + ':' +(case when (avg(milisPromedio)/(1000))/60 &gt; 10 then convert(varchar,(avg(milisPromedio)/(1000))/60) else '0'+convert(varchar,(avg(milisPromedio)/(1000))/60)  end )+ + ':' 
		+(case when (avg(milisPromedio)/(1000))%60 &gt; 10 then convert(varchar,(avg(milisPromedio)/(1000))%60) else '0'+convert(varchar,(avg(milisPromedio)/(1000))%60) end)promedio,null,null
		,convert(varchar,(avg(milisTiempo)/(1000))/(60*60)) + ':' +(case when (avg(milisTiempo)/(1000))/60 &gt; 10 then convert(varchar,(avg(milisTiempo)/(1000))/60) else '0'+convert(varchar,(avg(milisTiempo)/(1000))/60)  end )+ + ':' 
		+(case when (avg(milisTiempo)/(1000))%60 &gt; 10 then convert(varchar,(avg(milisTiempo)/(1000))%60) else '0'+convert(varchar,(avg(milisTiempo)/(1000))%60) end)promedio,null
		,convert(varchar,(avg(milisAtrasado)/(1000))/(60*60)) + ':' +(case when (avg(milisAtrasado)/(1000))/60 &gt; 10 then convert(varchar,(avg(milisAtrasado)/(1000))/60) else '0'+convert(varchar,(avg(milisAtrasado)/(1000))/60)  end )+ + ':' 
